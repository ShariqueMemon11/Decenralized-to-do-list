{
  "expo": {
    "name": "To-Do_list",
    "slug": "To-Do_list",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "todolist",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashimg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "wallet",
          "metamask",
          "trust",
          "rainbow",
          "argent"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.shariq1122.todolist",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "wc"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bc4d6dc0-186a-4b61-8521-ba48d892e861"
      }
    }
  }
}
